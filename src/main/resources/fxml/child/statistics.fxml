<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.*?>
<Pane xmlns:fx="http://javafx.com/fxml/1" minHeight="0.0" minWidth="0.0" prefHeight="454.0" prefWidth="912.0"
      xmlns="http://javafx.com/javafx/8" fx:controller="com.uz.laboratory.statistical.controller.StatisticsController">

    <children>
        <TableView fx:id="statisticsTableView" layoutX="-6.0" layoutY="156.0" prefHeight="319.0" prefWidth="920.0">
            <columns>
                <TableColumn prefWidth="131.0" text="Объект"/>
                <TableColumn minWidth="0.0" prefWidth="167.0" text="Замечание"/>
                <TableColumn prefWidth="282.0" text="Перегон"/>
                <TableColumn prefWidth="72.0" text="Дата"/>
                <TableColumn prefWidth="199.0" text="Вагон-лаборатория"/>
                <TableColumn prefWidth="68.0" text="Повторы"/>
            </columns>
        </TableView>
        <ComboBox fx:id="stage" layoutX="251.0" layoutY="39.0" prefHeight="25.0" prefWidth="274.0"/>
        <ComboBox fx:id="vagonLaboratory" layoutX="704.0" layoutY="39.0" prefHeight="25.0" prefWidth="190.0"/>
        <Label layoutX="355.0" layoutY="14.0" text="Перегон">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label layoutX="368.0" layoutY="79.0" text="Дата">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label layoutX="745.0" layoutY="14.0" text="Вагон-лаборатория">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label layoutX="45.0" layoutY="79.0" text="Направление движения">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Button fx:id="search" layoutX="546.0" layoutY="105.0" mnemonicParsing="false" onAction="#searchRemarksByFilter"
                prefHeight="30.0" prefWidth="90.0" text="Поиск" textAlignment="CENTER">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <ComboBox fx:id="sector" layoutX="14.0" layoutY="39.0" onAction="#currentSectorSelected" prefHeight="25.0"
                  prefWidth="215.0"/>
        <Label layoutX="100.0" layoutY="16.0" text="Участок"/>
        <ComboBox fx:id="deviceType" layoutX="540.0" layoutY="39.0" prefWidth="150.0"/>
        <Label layoutX="576.0" layoutY="15.0" text="Тип устройств"/>
        <ComboBox fx:id="directionOfMovement" layoutX="14.0" layoutY="107.0" prefHeight="25.0" prefWidth="214.0"/>
        <TextField fx:id="dateTextField" layoutX="251.0" layoutY="107.0" prefHeight="25.0" prefWidth="275.0"/>
        <Button fx:id="cleanTableView" layoutX="659.0" layoutY="105.0" mnemonicParsing="false"
                onAction="#cleanTableViewData" prefHeight="30.0" prefWidth="90.0" text="Очистить"
                textAlignment="CENTER">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Button fx:id="printAllTableViewDataFromExcel" layoutX="781.0" layoutY="105.0" mnemonicParsing="false"
                onAction="#printAllTableViewData" prefHeight="30.0" prefWidth="90.0" text="Печать"
                textAlignment="CENTER">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
    </children>
</Pane>
