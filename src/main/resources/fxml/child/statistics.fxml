<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.*?>
<Pane xmlns:fx="http://javafx.com/fxml/1" minHeight="0.0" minWidth="0.0" prefHeight="522.0" prefWidth="912.0"
      xmlns="http://javafx.com/javafx/8" fx:controller="com.uz.laboratory.statistical.controller.StatisticsController">

    <Pagination fx:id="statisticsTableViewPagination" layoutX="2.0" layoutY="188.0" prefHeight="275.0"
                prefWidth="912.0">
        <fx:define>
            <TableView fx:id="statisticsTableView" layoutX="2.0" layoutY="188.0" prefHeight="301.0" prefWidth="912.0">
                <placeholder>
                    <Label text="По результатам поиска ничего не найдено."/>
                </placeholder>
                <columns>
                    <TableColumn fx:id="objectColumn" prefWidth="131.0" text="Объект"/>
                    <TableColumn fx:id="noteColumn" minWidth="0.0" prefWidth="167.0" text="Замечание"/>
                    <TableColumn fx:id="stageColumn" prefWidth="282.0" text="Перегон"/>
                    <TableColumn fx:id="dateColumn" prefWidth="72.0" text="Дата"/>
                    <TableColumn fx:id="vagonColumn" prefWidth="133.0" text="Вагон-лаборатория"/>
                    <TableColumn fx:id="repeatColumn" prefWidth="125.0" text="Повторы"/>
                    <TableColumn fx:id="remarkIdColumn" minWidth="9.0" prefWidth="15.0" text="Id" visible="false"/>
                </columns>
            </TableView>
        </fx:define>
    </Pagination>
    <ComboBox fx:id="stageComboBox" layoutX="307.0" layoutY="32.0" prefHeight="25.0" prefWidth="255.0"/>
    <ComboBox fx:id="vagonLaboratoryComboBox" layoutX="744.0" layoutY="32.0" prefHeight="25.0" prefWidth="154.0"/>
    <Label layoutX="407.0" layoutY="7.0" text="Перегон">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="387.0" layoutY="71.0" text="Дата">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="767.0" layoutY="7.0" text="Вагон-лаборатория">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="15.0" layoutY="88.0" text="Направление движения">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Button fx:id="search" layoutX="565.0" layoutY="109.0" mnemonicParsing="false" onAction="#searchRemarksByFilter"
            prefHeight="30.0" prefWidth="90.0" text="Поиск" textAlignment="CENTER">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <ComboBox fx:id="sectorComboBox" layoutX="12.0" layoutY="32.0" onAction="#currentSectorSelected"
              prefHeight="25.0" prefWidth="275.0"/>
    <Label layoutX="128.0" layoutY="9.0" text="Участок"/>
    <ComboBox fx:id="deviceTypeComboBox" layoutX="578.0" layoutY="32.0" prefWidth="150.0"/>
    <Label layoutX="614.0" layoutY="9.0" text="Тип устройств"/>
    <ComboBox fx:id="directionOfMovementComboBox" layoutX="33.0" layoutY="130.0" prefHeight="25.0"
              prefWidth="118.0"/>
    <Button fx:id="cleanTableView" layoutX="685.0" layoutY="109.0" mnemonicParsing="false"
            onAction="#cleanTableViewData" prefHeight="30.0" prefWidth="90.0" text="Очистить"
            textAlignment="CENTER">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Button fx:id="printAllTableViewDataFromExcel" layoutX="799.0" layoutY="109.0" mnemonicParsing="false"
            onAction="#printAllTableViewData" prefHeight="30.0" prefWidth="90.0" text="Печать"
            textAlignment="CENTER">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <ComboBox fx:id="dayComboBox" layoutX="199.0" layoutY="113.0" prefHeight="25.0" prefWidth="66.0"/>
    <ComboBox fx:id="monthComboBox" layoutX="276.0" layoutY="113.0" onAction="#monthSelectedAction"
              prefHeight="25.0" prefWidth="118.0"/>
    <ComboBox fx:id="yearsComboBox" layoutX="402.0" layoutY="113.0" onAction="#yearSelectedAction" prefHeight="25.0"
              prefWidth="143.0"/>
    <Label layoutX="218.0" layoutY="92.0" text="День"/>
    <Label layoutX="317.0" layoutY="90.0" text="Месяц"/>
    <Label layoutX="464.0" layoutY="90.0" text="Год"/>
    <Separator layoutY="68.0" prefHeight="0.0" prefWidth="912.0"/>
    <Separator layoutX="290.0" orientation="VERTICAL" prefHeight="70.0" prefWidth="19.0"/>
    <Separator layoutX="562.0" orientation="VERTICAL" prefHeight="70.0" prefWidth="19.0"/>
    <Separator layoutX="726.0" layoutY="-1.0" orientation="VERTICAL" prefHeight="70.0" prefWidth="19.0"/>
    <Separator layoutX="180.0" layoutY="69.0" orientation="VERTICAL" prefHeight="119.0" prefWidth="6.0"/>
    <Separator layoutX="552.0" layoutY="68.0" orientation="VERTICAL" prefHeight="127.0" prefWidth="6.0"/>
    <Button fx:id="cleanDateComboBoxes" layoutX="342.0" layoutY="150.0" mnemonicParsing="false"
            onAction="#cleanDateComboBoxes" prefHeight="30.0" prefWidth="90.0" text="Очистить"
            textAlignment="CENTER">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</Pane>
