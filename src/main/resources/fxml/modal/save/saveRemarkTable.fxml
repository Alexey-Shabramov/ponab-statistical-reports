<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="151.0" prefWidth="488.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="com.uz.laboratory.statistical.controller.remark.SaveRemarkTableController">
    <children>
        <Button fx:id="saveRemarkTableButton" layoutX="111.0" layoutY="99.0" mnemonicParsing="false"
                onAction="#saveRemarkTableButtonAction" prefHeight="25.0" prefWidth="113.0" text="Cохранить">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="cancelSavingsButton" layoutX="265.0" layoutY="99.0" mnemonicParsing="false"
                onAction="#cancelSavingsButtonListener" prefHeight="25.0" prefWidth="113.0" text="Отмена">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <TextField fx:id="saveTableFolderPathTextField" editable="false" layoutX="17.0" layoutY="50.0" prefHeight="30.0"
                   prefWidth="316.0"/>
        <Button fx:id="chooseDirectoryButton" layoutX="351.0" layoutY="50.0" mnemonicParsing="false"
                onAction="#chooseDirectoryButtonListener" text="Выбрать папку">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Label layoutX="150.0" layoutY="14.0" text="Сохранить таблицы в файл">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
