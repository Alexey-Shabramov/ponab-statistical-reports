<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="82.0" prefWidth="294.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="com.uz.laboratory.statistical.controller.remark.PonabRemarkDeletionController">
    <Label layoutX="6.0" layoutY="7.0" text="Вы уверены в удалении этого элемента?">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Label>
    <Button fx:id="deleteConfirmButton" layoutX="17.0" layoutY="40.0" mnemonicParsing="false"
            onAction="#deleteConfirmButtonListener" prefHeight="25.0" prefWidth="106.0" text="Удалить">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Button fx:id="resetDeletionButton" layoutX="170.0" layoutY="40.0" mnemonicParsing="false"
            onAction="#resetDeletionButtonListener" prefHeight="25.0" prefWidth="106.0" text="Отменить">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</AnchorPane>
