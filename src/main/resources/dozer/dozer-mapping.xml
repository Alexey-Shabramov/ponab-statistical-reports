<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <configuration>
        <stop-on-errors>true</stop-on-errors>
        <date-format>MM/dd/yyyy HH:mm</date-format>
        <wildcard>true</wildcard>
    </configuration>

    <mapping map-id="ponabRemarkToDto">
        <class-a>com.uz.laboratory.statistical.entity.remark.PonabRemark</class-a>
        <class-b>com.uz.laboratory.statistical.dto.ponab.PonabRemarkDto</class-b>
    </mapping>

    <mapping map-id="cleanPonabRemarkDto">
        <class-a>com.uz.laboratory.statistical.dto.ponab.PonabRemarkDto</class-a>
        <class-b>com.uz.laboratory.statistical.dto.ponab.PonabRemarkDto</class-b>
    </mapping>

    <mapping map-id="cleanInspectionTripDto">
        <class-a>com.uz.laboratory.statistical.dto.trip.InspectionTripDto</class-a>
        <class-b>com.uz.laboratory.statistical.dto.trip.InspectionTripDto</class-b>
    </mapping>

    <mapping map-id="alsRemarkToDto">
        <class-a>com.uz.laboratory.statistical.entity.remark.AlsRemark</class-a>
        <class-b>com.uz.laboratory.statistical.dto.als.AlsRemarkDto</class-b>
    </mapping>

    <mapping map-id="cleanAlsRemarkDto">
        <class-a>com.uz.laboratory.statistical.dto.als.AlsRemarkDto</class-a>
        <class-b>com.uz.laboratory.statistical.dto.als.AlsRemarkDto</class-b>
    </mapping>

    <mapping map-id="ponabDeviceToDto">
        <class-a>com.uz.laboratory.statistical.entity.ponab.PonabSystem</class-a>
        <class-b>com.uz.laboratory.statistical.dto.ponab.PonabSystemDto</class-b>
    </mapping>

    <mapping map-id="alsDeviceToDto">
        <class-a>com.uz.laboratory.statistical.entity.als.TrackCircuit</class-a>
        <class-b>com.uz.laboratory.statistical.dto.als.AlsSystemDto</class-b>
    </mapping>

    <mapping map-id="inspectionTripToDto">
        <class-a>com.uz.laboratory.statistical.entity.trip.InspectionTrip</class-a>
        <class-b>com.uz.laboratory.statistical.dto.trip.InspectionTripDto</class-b>
    </mapping>

    <mapping map-id="cleanPonabDeviceDto">
        <class-a>com.uz.laboratory.statistical.dto.ponab.PonabSystemDto</class-a>
        <class-b>com.uz.laboratory.statistical.dto.ponab.PonabSystemDto</class-b>
    </mapping>

    <mapping map-id="cleanAlsDeviceDto">
        <class-a>com.uz.laboratory.statistical.dto.als.AlsSystemDto</class-a>
        <class-b>com.uz.laboratory.statistical.dto.als.AlsSystemDto</class-b>
    </mapping>
</mappings>